{
  "name": "nanoshooter",
  "version": "0.2.0-dev",
  "description": "robotic tank combat multiplayer action web game",
  "scripts": {
    "build": "rimraf o && mkdir o && browserify --debug -p tsify s/script.ts > o/script.bundle.js && tslint -e 's/_archive/**' 's/**/*.ts' && echo Done build",
    "test": "exit 0",
    "prepare": "npm run build -s && npm test -s",
    "start": "http-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChaseMoskal/Nanoshooter.git"
  },
  "author": "Chase Moskal <chasemoskal@gmail.com>",
  "private": true,
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChaseMoskal/Nanoshooter/issues"
  },
  "homepage": "https://github.com/ChaseMoskal/Nanoshooter#readme",
  "devDependencies": {
    "browserify": "14.4.0",
    "http-server": "0.10.0",
    "rimraf": "2.6.1",
    "tsify": "3.0.1",
    "tslint": "5.5.0",
    "typescript": "2.4.1"
  },
  "dependencies": {
    "babylonjs": "3.0.4-beta",
    "monarch-engine": "0.1.0"
  }
}
